{
  "name": "um-resource-scraper",
  "version": "0.1.0",
  "description": "A web scraper for obtaining the resources of your profile in the University of Murcia.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "parcel build index.html --public-url ./",
    "dev": "npm run build && npm start",
    "lint": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dizzyrobin/um-resource-scraper.git"
  },
  "keywords": [
    "um",
    "resouce",
    "scraper"
  ],
  "author": "dizzyrobin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dizzyrobin/um-resource-scraper/issues"
  },
  "homepage": "https://github.com/dizzyrobin/um-resource-scraper#readme",
  "dependencies": {
    "electron": "^2.0.8",
    "node-fetch": "^2.2.0",
    "postcss-modules": "^1.3.2",
    "prop-types": "^15.6.2",
    "puppeteer": "^1.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "parcel-bundler": "^1.9.7",
    "xo": "^0.22.0"
  },
  "xo": {
    "extends": "xo-react",
    "envs": [
      "browser"
    ],
    "space": 2,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
